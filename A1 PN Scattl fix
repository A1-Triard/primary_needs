SCPT
SCHD Bal_check_morsel 0 0 0 1023 0
SCDT 
SCTX
    Begin Bal_check_morsel
    
    	stopscript Bal_check_morsel
    	set found_food to 0
    	if ( GetItemCount "ingred_scuttle_01" > 0 )
    		set found_food to 1
    		return
    	endif
    	if ( GetItemCount "deepnight_travelFood" > 0 )
    		set found_food to 1
    		return
    	endif
    	if ( GetItemCount "ingred_bread_01" > 0 )
    		set found_food to 1
    		return
    	endif
    	if ( GetItemCount "food_kwama_egg_01" > 0 )
    		set found_food to 1
    		return
    	endif
    	if ( GetItemCount "ingred_saltrice_01" > 1 )
    		set found_food to 1
    		return
    	endif
    	if ( GetItemCount "food_kwama_egg_02" > 0 )
    		set found_food to 1
    		return
    	endif
    	if ( GetItemCount "ingred_ash_yam_01" > 1 )
    		set found_food to 1
    		return
    	endif
    	if ( GetItemCount "ingred_scrib_jerky_01" > 0 )
    		set found_food to 1
    		return
    	endif
    	if ( GetItemCount "ingred_scrib_jelly_01" > 0 )
    		set found_food to 1
    		return	
    	endif
    	if ( GetItemCount "ingred_corkbulb_root_01" > 0 )
    		set found_food to 1
    		return	
    	endif
    	if ( GetItemCount "ingred_comberry_01" > 1 )
    		set found_food to 1
    		return	
    	endif
    	if ( GetItemCount "ingred_crab_meat_01" > 0 )
    		set found_food to 1
    		return	
    	endif
    	if ( GetItemCount "ingred_hound_meat_01" > 0 )
    		set found_food to 1
    		return	
    	endif
    	if ( GetItemCount "ingred_human_meat_01" > 0 )
    		set found_food to 1
    		return	
    	endif
    	if ( GetItemCount "ingred_kagouti_hide_01" > 0 )
    		set found_food to 1
    		return	
    	endif
    	if ( GetItemCount "ingred_muck_01" > 1 )
    		set found_food to 1
    		return	
    	endif
    	if ( GetItemCount "ingred_rat_meat_01" > 0 )
    		set found_food to 1
    		return	
    	endif
    	if ( GetItemCount "ingred_trama_root_01" > 0 )
    		set found_food to 1
    		return	
    	endif
    
    end

SCPT
SCHD Bal_dump_morsel 0 0 0 1126 0
SCDT 
SCTX
    Begin Bal_dump_morsel
    StopScript Bal_dump_morsel
    ;Initially created by Shanjak
    	if ( GetItemCount "ingred_scuttle_01" > 0 )
    		RemoveItem "ingred_scuttle_01" 1
    		return
    	endif
    	if ( GetItemCount "deepnight_travelFood" > 0 )
    		RemoveItem "deepnight_travelFood" 1
    		return
    	endif
    	if ( GetItemCount "ingred_bread_01" > 0 )
    		RemoveItem "ingred_bread_01" 1
    		return
    	endif
    	if ( GetItemCount "food_kwama_egg_01" > 0 )
    		RemoveItem "food_kwama_egg_01" 1
    		return
    	endif
    	if ( GetItemCount "ingred_saltrice_01" > 1 )
    		RemoveItem "ingred_saltrice_01" 2
    		return
    	endif
    	if ( GetItemCount "food_kwama_egg_02" > 0 )
    		RemoveItem "food_kwama_egg_02" 1
    		return
    	endif
    	if ( GetItemCount "ingred_ash_yam_01" > 1 )
    		RemoveItem "ingred_ash_yam_01" 2
    		return
    	endif
    	if ( GetItemCount "ingred_scrib_jerky_01" > 0 )
    		RemoveItem "ingred_scrib_jerky_01" 1
    		return
    	endif
    	if ( GetItemCount "ingred_scrib_jelly_01" > 0 )
    		RemoveItem "ingred_scrib_jelly_01" 1
    		return
    	endif
    	if ( GetItemCount "ingred_corkbulb_root_01" > 0 )
    		RemoveItem "ingred_corkbulb_root_01" 1
    		return
    	endif
    	if ( GetItemCount "ingred_comberry_01" > 1 )
    		RemoveItem "ingred_comberry_01" 2
    		return
    	endif
    	if ( GetItemCount "ingred_crab_meat_01" > 0 )
    		RemoveItem "ingred_crab_meat_01" 1
    		return
    	endif
    	if ( GetItemCount "ingred_hound_meat_01" > 0 )
    		RemoveItem "ingred_hound_meat_01" 1
    		return
    	endif
    	if ( GetItemCount "ingred_human_meat_01" > 0 )
    		RemoveItem "ingred_human_meat_01" 1
    		return
    	endif
    	if ( GetItemCount "ingred_kagouti_hide_01" > 0 )
    		RemoveItem "ingred_kagouti_hide_01" 1
    		return
    	endif
    	if ( GetItemCount "ingred_muck_01" > 1 )
    		RemoveItem "ingred_muck_01" 2
    		return
    	endif
    	if ( GetItemCount "ingred_rat_meat_01" > 0 )
    		RemoveItem "ingred_rat_meat_01" 1
    		return
    	endif
    	if ( GetItemCount "ingred_trama_root_01" > 0 )
    		RemoveItem "ingred_trama_root_01" 1
    		return
    	endif
    End
    

SCPT
SCHD Bal_eat_morsel 0 0 0 2252 0
SCDT 
SCTX
    Begin Bal_eat_morsel
    StopScript Bal_eat_morsel
    ;Initially created by Shanjak
    
    	if ( GetItemCount "ingred_scuttle_01" > 0 )
    		MessageBox, "Вы съели порцию скаттла, чтобы утолить голод."
    		set ToEat to 1
    		playSound "Swallow"
    		return
    	endif
    	if ( GetItemCount "deepnight_travelFood" > 0 )
    		MessageBox, "Вы съели порцию походного рациона, дабы заморить червячка."
    		set ToEat to 1
    		playSound "Swallow"
    		return
    	endif
    	if ( GetItemCount "ingred_bread_01" > 0 )			
    		MessageBox, "Вы замяли ломоть хлеба, чтобы утолить голод."
    		set ToEat to 1
    		playSound "Swallow"
    		return
    	endif
    	if ( GetItemCount "food_kwama_egg_01" > 1 )			
    		MessageBox, "Вы закинули маленькое яйцо квама, чтобы немного подкрепиться."
    		set ToEat to 1
    		playSound "Swallow"
    		return
    	endif
    	if ( GetItemCount "ingred_saltrice_01" > 1 )			
    		MessageBox, "Вы умяли немного соленого риса, дабы подкрепить расшатанное здоровье."
    		set ToEat to 1
    		playSound "Swallow"
    		return
    	endif
    	if ( GetItemCount "food_kwama_egg_02" > 0 )			
    		MessageBox, "Вы проглотили большое яйцо квама, чтобы удовлетворить ваш голод."
    		set ToEat to 2
    		playSound "Swallow"
    		return
    	endif
    	if ( GetItemCount "ingred_ash_yam_01" > 1 )			
    		MessageBox, "Вы убрали два сладких пепельных батата, для подавления голода."
    		set ToEat to 1
    		playSound "Swallow"
    		return
    	endif
    	if ( GetItemCount "ingred_scrib_jerky_01" > 0 )			
    		MessageBox, "Вы зажевали вяленную скрибятину, чтобы удовлетворить ваш растущий аппетит."
    		set ToEat to 1
    		playSound "Swallow"
    		return
    	endif
    	if ( GetItemCount "ingred_scrib_jelly_01" > 0 )			
    		MessageBox, "Вы поглотили немного скрибового желе, для подавления голода."
    		set ToEat to 1
    		playSound "Swallow"
    		return
    	endif
    	if ( GetItemCount "ingred_corkbulb_root_01" > 0 )			
    		MessageBox, "Вы давились жестким пробочником, чтобы слегка насытить урчащий желудок."
    		set ToEat to 1
    		playSound "Swallow"
    		return
    	endif
    	if ( GetItemCount "ingred_comberry_01" > 1 )			
    		MessageBox, "Вы запитались парой веточек комимуники, для сладости и сытости."
    		set ToEat to 1
    		playSound "Swallow"
    		return
    	endif
    	if ( GetItemCount "ingred_crab_meat_01" > 0 )			
    		MessageBox, "Вы потребили деликатесное крабовое мясо. Это было и вкусно и сытно."
    		set ToEat to 1
    		playSound "Swallow"
    		return
    	endif
    	if ( GetItemCount "ingred_hound_meat_01" > 0 )
    		MessageBox, "Вы зажевали сального мяса гончей, чтобы подавить голод."
    		set ToEat to 1
    		playSound "Swallow"
    		return
    	endif
    	if ( GetItemCount "ingred_human_meat_01" > 0 )			
    		MessageBox, "Вы давитесь человеченой, чтобы задушить голод."
    		set ToEat to 1
    		playSound "Swallow"
    		return
    	endif
    	if ( GetItemCount "ingred_kagouti_hide_01" > 0 )			
    		MessageBox, "Вы грызете кожу кагути, чтоб как-то уморить голод."
    		set ToEat to 1
    		playSound "Swallow"
    		return
    	endif
    	if ( GetItemCount "ingred_muck_01" > 1 )			
    		MessageBox, "Вы задегустировали муск, голод не тетка."
    		set ToEat to 1
    		playSound "Swallow"
    		return
    	endif
    	if ( GetItemCount "ingred_rat_meat_01" > 0 )			
    		MessageBox, "Вы проглотили вонючего крысиного мяса, и голод отступил."
    		set ToEat to 1
    		playSound "Swallow"
    		return
    	endif
    	if ( GetItemCount "ingred_trama_root_01" > 0 )			
    		MessageBox, "Вы прибегаете к уптреблению корня трамы, только б задавить голод."
    		set ToEat to 1
    		playSound "Swallow"
    		return
    	endif
    End
    
