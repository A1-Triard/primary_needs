SCHD SleepDetect 0 0 1 114 6
SCVR timer;;
SCDT BgEBCSBY4BAgPT0gMQUBRwtQbGF5ZXJTbGVwdAIgMQgBBRMgRwtQbGF5ZXJTbGVwdCA9PSAxBQFmAQAKIGYBACBYEhAgKwYBAgggZgEAID4gMQUBZgEAAiAwBQFHC1BsYXllclNsZXB0AiAwCQEJAQEB
SCTX
    begin SleepDetect
    ; That script detects and flags if player has rested.
    float timer
    
    if ( GetPCSleep == 1 )
    	set PlayerSlept to 1 ;Global flag to show my scripts if player has rested.
    elseif ( PlayerSlept == 1)
    	set timer to timer + GetSecondsPassed
    	if ( timer > 1 )
    		set timer to 0
    		set PlayerSlept to 0
    	endif
    endif
    
    end SleepDetect

