SCHD Bal_bedroll_script 3 0 0 331 24
SCVR choice;MessageOn;chosen;;
SCDT 
SCTX
    begin Bal_bedroll_script
    
    short choice
    short MessageOn
    short chosen
    
    if ( MenuMode == 1 )
    	return
    endif
    
    if (OnActivate == 1)
    	Messagebox "Как Вы хотите использовать спальник?",  "Спать", "Свернуть", "Ничего"
    	set MessageOn to 1
    	set chosen to 1
    endif
    
    if (messageon == 1)
    	set choice to GetButtonPressed
    endif
    
    if (choice == 0)
    if (chosen == 1)
    	ShowRestMenu
    	set MessageOn to 0
    	set choice to 3
    	set sleep_mod to 1
    endif
    endif
    
    if (choice == 1)
    	player->AddItem, "Balors_portable_bedroll", 1
    	playsound "item misc up"
    	disable	
    	setdelete 1
    	set choice to 3
    	set MessageOn to 0
    endif
    endif
    
    if (choice == 2)
    	set choice to 3
    	set MessageOn to 0
    	return
    endif
    
    end

