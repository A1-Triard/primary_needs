SCDT BgEOECBHCFBDSW5KYWlsID09IDEGAQIJIHMCACA9PSAwBQFzAQAIIEcERGF5AAAFAXMCAAIgMQkBBgEEEyBHDEphaWxEYXlzTGVmdCA+IDAGAQIPIHMBACAhPSBHBERheQAABQFzAQAIIEcERGF5AAAFAUcMSmFpbERheXNMZWZ0EyBHDEphaWxEYXlzTGVmdCAxIC0JAQcBAwUBRwhQQ0luSmFpbAIgMAUBcwIAAiAwHBAGUENKYWlsCQEJAQEB
SCTX
    begin PCJail
    
    Short CurrentDay
    Short DoOnce
    
    if ( PCInJail == 1 )
    	if ( DoOnce == 0 )
    		set CurrentDay to Day
    		set DoOnce to 1
    	endif
    	if ( JailDaysLeft > 0 )
    		if ( CurrentDay != Day )
    			set CurrentDay to Day
    			set JailDaysLeft to ( JailDaysLeft - 1 )
    		endif
    	else
    		set PCInJail to 0
    		set DoOnce to 0
    		StopScript "PCJail"
    	endif
    endif
    
    End PCJail

