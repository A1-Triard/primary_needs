SCHD deepnight_campement 13 1 1 3110 129
SCVR installed;doOnce;campState;choix;heure;mange;onPCdrop;weatherOnce;drinkOnce;PCskipEquip;onPCequip;BOOST_SLEEP;BOOST_MEAL;i;timer;;
SCDT 
SCTX
    begin deepnight_campement
    
    short installed
    short doOnce
    short campState
    short choix
    float timer
    short heure
    short mange
    short onPCdrop
    short weatherOnce
    short drinkOnce
    short PCskipEquip
    short onPCequip
    long i
    short BOOST_SLEEP
    short BOOST_MEAL
    
    
    ; Variable а ajuster pour booster ou non la guйrison par le camp
    set BOOST_SLEEP to 5
    
    if (onPCequip==1)
    	MessageBox "Опустите рюкзак на землю, чтобы разбить ваш лагерь."
    	set ToEat to 17
    	set onPCequip to 0
    	return
    endif
    
    ; --- ON OVERRIDE LES ROUTINES DE SOMMEIL
    if ( campState==4 )
    	
    	if ( GetDistance, Player > 512 )
    		set campState to 0
    		return 
    	endif
    
    	; Quand la boite de message de Repos de Morrowind est annulйe, le campState vaut toujours 6... Ce IF sert а corriger ca.
    	if ( onActivate==1 )
    		set campState to 0
    	else
    		if ( GetPCSleep==1 )
    			if (doOnce==0)
    				messageBox "Ваш отдых в высшей степени крут! :)"
    				set doOnce to 1
    			endif
    			set timer to timer + 1
    			if (timer>4)
    				Player->ModCurrentHealth, BOOST_SLEEP
    				Player->ModCurrentMagicka, BOOST_SLEEP
    				Player->ModCurrentFatigue, BOOST_SLEEP
    				set timer to 0
    			endif
    		else
    			if ( doOnce==1 )
    				set doOnce to 0
    				set campState to 0
    				return
    			endif
    		endif
    	endif
    
    endif
    
    
    if (menuMode==1)
    	return
    endif
    
    
    
    ; --- ANNULATION DU DEPLOIEMENT DU CAMP QUAND IL PLEUT
    if ( campState==0 )
    	set PCskipEquip to 1
    	if ( onPCdrop==1 )
    		if ( GetCurrentWeather > 3 )
    			MessageBox "Вы не можете разбить лагерь в плохую погоду!"
    			Activate
    		else
    			MessageBox "Вы укладываете лагерь."
    			set installed to 1
    		endif
    		set onPCdrop to 0
    	endif
    endif
    
    if (installed==0)
    	return
    endif
    		
    ; --- PLIAGE DU CAMP
    if ( campState==-1)
    	set installed to 0
    	MessageBox "Вы сворачиваете лагерь."
    	set campState to 0
    	PlaySound "Torch Out"
    	Activate
    	return
    endif
    
    
    ; --- ACTIVATION DU CAMP AU SOL
    if ( campState==0 )
    	if ( onActivate==1 )
    		MessageBox "Час: %.0f, День: %.0f . Что Вы хотите сделать ?"  GameHour Day "Отдохнуть" "Съесть что-нибудь" "Свернуть лагерь" "ОТМЕНА"
    		set campState to 2
    		set choix to -1
    	endif
    endif
    
    
    ; --- VERIF DES REPONSES
    if ( campState==2 )
    	set choix to GetButtonPressed
    	if (choix==0)
    		set campState to 3
    	endif
    
    	if (choix==1)
    		set campState to 5
    	endif
    
    	if (choix==2)
    		set campState to -1
    	endif
    
    	if (choix==3)
    		set campState to 0
    	endif
    
    endif
    
    
    ; --- CHOIX "SE REPOSER"
    if ( campState==3)
    	set campState to 4
    	set timer to 0
    	set i to 0
    	showRestMenu
    endif
    
    
    ; --- BEIGNET AU COIN DU FEU, CHOIX DU MENU
    if ( campState==5 )
    	MessageBox "Что Вы хотите приготовить ?" "Походный рацион" "Немного хлеба" "Маленькое яйцо квама" "Большое яйцо квама" "Краба на гриле" "Вереск" "Соленый рис" "'Шефф салат'" "ОТМЕНА"
    	set campState to 6
    	return
    endif
    
    
    ; --- BEIGNET AU COIN DU FEU, REPAS
    if ( campState==6 )
    
    	set choix to GetButtonPressed
    	set mange to 0
    
    	if (choix==0)
    		if ( Player->GetItemCount, "deepnight_travelFood" > 0 )
    			Player->RemoveItem, "deepnight_travelFood", 1
    			MessageBox "Вы употребили Походный рацион."
    			set mange to 1
    			set ToEat to 1
    			set BOOST_MEAL to 20
    		endif
    	endif
    
    	if (choix==1)
    		If ( Player->GetItemCount, "ingred_bread_01" > 0 )
    			Player->RemoveItem, "ingred_bread_01", 1
    			MessageBox "Ва нажарили немного хлеба."
    			set mange to 1
    			set ToEat to 1
    			set BOOST_MEAL to 10
    		endif
    	endif
    
    	if (choix==2)
    		If ( Player->GetItemCount, "food_kwama_egg_01" > 0 )
    			Player->RemoveItem, "food_kwama_egg_01", 1
    			MessageBox "Вы сварганили маленький омлет квама."
    			set BOOST_MEAL to 30
    			set ToEat to 2
    			set mange to 1
    		endif
    	endif
    
    	if (choix==3)
    		If ( Player->GetItemCount, "food_kwama_egg_02" > 0 )
    			Player->RemoveItem, "food_kwama_egg_02", 1
    			MessageBox "Вы оприходовали большой омлет квама с травами."
    			set BOOST_MEAL to 50
    			set ToEat to 4
    			set mange to 1
    		endif
    	endif
    					
    	if (choix==4)
    		If ( Player->GetItemCount, "ingred_crab_meat_01" > 0 )
    			Player->RemoveItem, "ingred_crab_meat_01", 1
    			MessageBox "Вы сделали восхитительного краба на гриле."
    			set BOOST_MEAL to 65
    			set ToEat to 10
    			set mange to 1
    		endif
    	endif
    
    	if (choix==5)
    		If ( Player->GetItemCount, "ingred_heather_01" > 0 )
    			Player->RemoveItem, "ingred_heather_01", 1
    			MessageBox "Вы сделали пекрасный вересковый салат."
    			set BOOST_MEAL to 25
    			set ToEat to 16
    			set mange to 1
    		endif
    	endif
    
    	if (choix==6)
    		If ( Player->GetItemCount, "ingred_saltrice_01" > 0 )
    			Player->RemoveItem, "ingred_saltrice_01", 1
    			MessageBox "Вы приготовили немного гарнира изсоленого риса."
    			set BOOST_MEAL to 40
    			set ToEat to 3
    			set mange to 1
    		endif
    	endif
    
    	if (choix==7)
    		If ( Player->GetItemCount, "ingred_heather_01" > 1 )
    			If ( Player->GetItemCount, "food_kwama_egg_01" > 0 )
    				If ( Player->GetItemCount, "ingred_gold_kanet_01" > 0 )
    					Player->RemoveItem, "ingred_heather_01", 2
    					Player->RemoveItem, "ingred_gold_kanet_01", 1
    					Player->RemoveItem, "food_kwama_egg_01", 1
    					MessageBox "Вы затратили некоторое время, чтобы делать отменный Шеф салат со всеми необходимыми компонентами. Выглядит действительно хорошо !"
    					set BOOST_MEAL to 9999
    					set ToEat to 2
    					set mange to 1
    				endif
    			endif
    		endif
    	endif
    
    
    	if (choix==8)
    		set campState to 0
    		return
    	endif
    
    	if (mange==0)
    		if (choix==7)
    			MessageBox "Вы нужно 2 вереска, 1 маленькое яйцо квама и 1 золотой канет, чтобы приготовить Шеф салат."
    		else
    			MessageBox "Жаль, но у Вас этого нет."
    		endif
    	else
    		set timer to 0
    		DisablePlayerControls
    		fadeOut 1
    		set campState to 9
    		return
    	endif
    
    	set campState to 0
    endif
    
    
    if ( campState==9)
    	set timer to timer+GetSecondsPassed
    
    	if (timer>2)
    		if ( drinkOnce==0 )
    			PlaySound "drink"
    			set drinkOnce to 1
    			return
    		endif
    	endif
    	if (timer>4)
    		set drinkOnce to 0
    		fadeIn 1
    		EnablePlayerControls
    		PlaySound "Swallow"
    		MessageBox "Вы чувствуете себя лучше."
    		Player->ModCurrentHealth, BOOST_MEAL
    		Player->ModCurrentFatigue, BOOST_MEAL
    		set campState to 0
    		set timer to 0
    	endif
    endif
    
    end

