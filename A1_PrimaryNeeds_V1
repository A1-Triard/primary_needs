    if ( doDisable > 20 )
    	if ( GetDisabled == 1 )
    		set temp to 2 * ( A1V6_RoadBagMiscExt / 2 )
    		set A1V6_RoadBagMiscExt to temp
    		SetDelete 1
    		return
    	endif
    endif
    
    if ( doDisable > 20 )
    	Disable
    	return
    endif
    
