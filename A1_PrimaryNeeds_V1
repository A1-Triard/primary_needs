SCPT
SCHD A1V6_RoadBagAlchCont_1_sc 7 8 5 1435 152
SCVR doOnce;doRemove;doDisable;temp;temp2;doActivate;waitActivate;invItem;invCount;invRef;pcRef;invRefType;tempLong;invName;invNextRef;cx;cy;cz;ca;invWeight;;
SCDT 
SCTX
    Begin A1V6_RoadBagAlchCont_1_sc
    
    short doOnce
    short doRemove
    short doDisable
    short temp
    short temp2
    float cx
    float cy
    float cz
    float ca
    long invItem
    long invCount
    long invRef
    long pcRef
    long invRefType
    long tempLong
    float invWeight
    long invName
    long invNextRef
    short doActivate
    short waitActivate
    
    if ( doDisable > 20 )
    	if ( GetDisabled == 1 )
    		set temp to 2 * ( A1V6_RoadBagAlchExt / 2 )
    		set A1V6_RoadBagAlchExt to temp
    		SetDelete 1
    		return
    	endif
    endif
    
    if ( doDisable > 20 )
    	Disable
    	return
    endif
    
    if ( doDisable > 0 )
    	AddItem "A1V6_RoadBagFakeIngred", 2
    	set doDisable to ( doDisable + 1 )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	Enable
    	set doDisable to 1
    endif
    
    if ( waitActivate == 2 )
    	if ( MenuMode == 0 )
    		set waitActivate to 0
    		if ( GetItemCount A1V6_RoadBagAlchItem_1 == 0 )
    			set doDisable to 1
    		endif
    	endif
    	return
    elseif ( waitActivate == 1 )
    	if ( MenuMode == 1 )
    		set waitActivate to 2
    	endif
    	return
    endif
    
    if ( MenuMode == 1 )
    	return
    endif
    
    ifx ( doOnce )
    else
    	set temp to 2 * ( A1V6_RoadBagAlchExt / 2 ) + 1
    	set A1V6_RoadBagAlchExt to temp
    	DisablePlayerControls
    	set doOnce to 1
    	set cx to A1V6_RoadBagX
    	set cy to A1V6_RoadBagY
    	set cz to A1V6_RoadBagZ
    	set ca to A1V6_RoadBagAngle
    	SetPos X cx
    	SetPos Y cy
    	SetPos Z cz
    	SetAngle Z ca
    	setx invItem,invCount,invRef to xInventory
    	whilex ( invCount )
    		xRemoveItem invItem invCount
    		setx invItem,invCount,invRef to xInventory
    	endwhile
    	AddItem "A1V6_RoadBagAlchItem_1", 1
    	set invRef to 0
    	set doRemove to 1
    	return
    endif
    
    if ( doActivate == 1 )
    	set waitActivate to 1
    	set doActivate to 0
    	Activate
    	return
    endif
    
    if ( doRemove == 10 )
    	set doRemove to 0
    	MessageBox "Зелья выложены"
    	EnablePlayerControls
    	return
    endif
    
    if ( doRemove > 5 )
    	set temp to 1
    else
    	set temp to 0
    endif
    
    ifx ( temp )
    	setx pcRef to xGetRef "player"
    	whilex ( pcRef )
    		setx invItem,invCount,invRefType,tempLong,invWeight,invName,invNextRef to pcRef->xContentList invRef
    		ifx ( invItem )
    			if ( invRefType == 1212369985 )
    				set temp to 1
    			else
    				set temp to 0
    			endif
    			ifx ( temp )
    				setx tempLong to xStringCompare invName, "1/"
    				if ( tempLong > 0 )
    					set temp2 to 1
    				else
    					set temp2 to 0
    				endif
    				ifx ( temp2 )
    					setx tempLong to xStringCompare invName, "1/a"
    					if ( tempLong < 0 )
    						set temp to 0
    					endif
    				endif
    			endif
    			ifx ( temp )
    				pcRef->xRemoveItem invItem invCount
    				xAddItem invItem invCount
    				set invRef to 0
    				set pcRef to 0
    				set doRemove to 1
    			endif
    		endif
    		if ( doRemove > 1 )
    			set invRef to invNextRef
    		endif
    		if ( invNextRef == 0 )
    			set pcRef to 0
    			set doRemove to 10
    		endif
    	endwhile
    	return
    endif
    
    if ( doRemove > 0 )
    	set doRemove to ( doRemove + 1 )
    	return
    endif
    
    if ( OnActivate == 1 )
    	set doActivate to 1
    	return
    endif
    
    End

