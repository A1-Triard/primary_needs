    	if ( MenuMode == 0 )
    		set waitActivate to 0
    		if ( GetItemCount A1V6_RoadBagMiscItem_3 == 0 )
    			set doDisable to 1
    		endif
    	endif
    	return
    elseif ( waitActivate == 1 )
    	if ( MenuMode == 1 )
    		set waitActivate to 2
    	endif
    	return
    endif
    
    if ( MenuMode == 1 )
    	return
    endif
    
    ifx ( doOnce )
    else
    	set temp to A1V6_RoadBagMiscExt / 4
    	set temp to A1V6_RoadBagMiscExt - 4 * temp + 4
    	set A1V6_RoadBagMiscExt to temp
