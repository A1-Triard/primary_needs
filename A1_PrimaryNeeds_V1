    	set temp1 to temp1 / 2									; temp1 = (flags div 2) div 2 ~ flags & 0b100
    	if ( temp2 == 0 )
    		player->AddItem A1V6_RoadBagAlchItem_1, 1
    	elseif ( temp1 == 0 )
