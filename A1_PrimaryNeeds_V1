    	set temp2 to A1V6_RoadBagMiscExt - 2 * temp1	; temp2 = flags - 2 (flags div 2) = flags mod 2 ~ flags & 0b001
    	set temp1 to temp1 / 2									; temp1 = (flags div 2) div 2 ~ flags & 0b100
    	if ( temp2 == 0 )
    		player->AddItem A1V6_RoadBagMiscItem_1, 1
    	elseif ( temp1 == 0 )
    		player->AddItem A1V6_RoadBagMiscItem_3, 1
