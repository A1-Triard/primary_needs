SCHD A1V6_RoadBagIngrItem_1_sc 6 0 0 809 44
SCVR wait;OnPCEquip;OnPCAdd;OnPCDrop;temp1;temp2;;
SCDT 
SCTX
    Begin A1V6_RoadBagIngrItem_1_sc
    
    short wait
    short OnPCEquip
    short OnPCAdd
    short OnPCDrop
    short temp1
    short temp2
    
    if ( GetDisabled == 1 )
    	SetDelete 1
    	return
    endif
    
    if ( wait == 1 )
    	set temp1 to GetButtonPressed
    	if ( temp1 == 0 )
    		set wait to 0
    		StartScript A1V6_RoadBagIngrDel_1_sc
    	elseif ( temp1 == 1 )
    		set wait to 0
    	endif
    	return
    endif
    
    if ( OnPCAdd == 1 )
    	set OnPCAdd to 0
    	if ( player->GetItemCount A1V6_RoadBagIngrItem_2 == 1 )
    		player->RemoveItem A1V6_RoadBagIngrItem_2, 1
    	endif
    	if ( player->GetItemCount A1V6_RoadBagIngrItem_3 == 1 )
    		player->RemoveItem A1V6_RoadBagIngrItem_3, 1
    	endif
    endif
    
    if ( OnPCDrop == 1 )
    	set OnPCDrop to 0
    	set A1V6_RoadBagX to GetPos X
    	set A1V6_RoadBagY to GetPos Y
    	set A1V6_RoadBagZ to GetPos Z
    	PlaceAtPC A1V6_RoadBagIngrCont_1 1, 50, 0
    	set temp1 to A1V6_RoadBagIngrExt / 2				; temp1 = flags div 2
    	set temp2 to temp1 / 2									; temp2 = (flags div 2) div 2 ~ flags & 0b100
    	set temp1 to temp1 - 2 * temp2							; temp1 = flags div 2 - 2 ((flags div 2) div 2) = (flags div 2) mod 2 ~ flags & 0b010
    	if ( temp1 == 0 )
    		player->AddItem A1V6_RoadBagIngrItem_2, 1
    	elseif ( temp2 == 0 )
    		player->AddItem A1V6_RoadBagIngrItem_3, 1
    	endif
    	Disable
    endif
    
    if ( OnPCEquip == 1 )
    	set OnPCEquip to 0
    	if ( A1V6_RoadBagIngrExt == 0 )
    		MessageBox "Вы действительно не хотите пользоваться мешком для ингредиентов?" "Да" "Нет"
    		set wait to 1
    	else
    		MessageBox "По миру разбросаны ваши мешки с ингредиентами."
    	endif
    endif
    
    End

