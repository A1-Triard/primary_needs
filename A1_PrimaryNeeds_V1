    	set OnPCEquip to 0
    	if ( A1V6_RoadBagMiscExt == 0 )
    		MessageBox "Вы действительно не хотите пользоваться походным мешком?" "Да" "Нет"
    		set wait to 1
    	else
    		MessageBox "По миру разбросаны ваши походные мешки."
    	endif
    endif
    
    End

SCPT
SCHD A1V6_StartContainers 1 0 0 192 7
SCVR doOnce;;
SCDT BgEFCSBzAQAgPT0gMAUBcwEAAiAxDAEGcGxheWVy1BAWQTFWNl9Sb2FkQmFnQWxjaEl0ZW1fMQEADAEGcGxheWVy1BAWQTFWNl9Sb2FkQmFnSW5jcEl0ZW1fMQEADAEGcGxheWVy1BAWQTFWNl9Sb2FkQmFnSW5nckl0ZW1fMQEADAEGcGxheWVy1BAWQTFWNl9Sb2FkQmFnTWlzY0l0ZW1fMQEACQEcEBRBMVY2X1N0YXJ0Q29udGFpbmVycwEB
SCTX
    Begin A1V6_StartContainers
    
    short doOnce
    
    if ( doOnce == 0 )
    	set doOnce to 1
    	player->AddItem A1V6_RoadBagAlchItem_1, 1
    	player->AddItem A1V6_RoadBagIncpItem_1, 1
    	player->AddItem A1V6_RoadBagIngrItem_1, 1
    	player->AddItem A1V6_RoadBagMiscItem_1, 1
    endif
    
    StopScript A1V6_StartContainers
    
    End

