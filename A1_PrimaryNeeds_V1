    if ( doDisable > 20 )
    	if ( GetDisabled == 1 )
    		set temp to A1V6_RoadBagMiscExt / 4
    		set temp to A1V6_RoadBagMiscExt - 4 * temp
    		set A1V6_RoadBagMiscExt to temp
    		SetDelete 1
    		return
    	endif
    endif
    
