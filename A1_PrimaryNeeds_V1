SCHD A1V6_RoadBagAlchDel_1_sc 0 0 0 114 0
SCDT DAEGcGxheWVyBgEBIiBY/RAgbxZBMVY2X1JvYWRCYWdBbGNoSXRlbV8xID09IDEMAQZwbGF5ZXLVEBZBMVY2X1JvYWRCYWdBbGNoSXRlbV8xAQAJARwQGEExVjZfUm9hZEJhZ0FsY2hEZWxfMV9zYwEB
SCTX
    Begin A1V6_RoadBagAlchDel_1_sc
    
    if ( player->GetItemCount A1V6_RoadBagAlchItem_1 == 1 )
    	player->RemoveItem A1V6_RoadBagAlchItem_1, 1
    endif
    
    StopScript A1V6_RoadBagAlchDel_1_sc
    
    End

